<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:ads="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="wt.ptrngame.levelEleven"
    android:id="@+id/content"
    android:background="#b4fbac"
    >

    <Button
        android:id="@+id/angry_btn"

        android:text="Back"
        android:textColor="#1f9213"
        android:textSize="18sp"

        android:layout_width="80dp"
        android:layout_height="40dp"
        android:background="@drawable/backbuttongreen"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:onClick="BackButton"/>


    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:id="@+id/textView16"
        android:layout_centerVertical="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:text="              " />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:textColor="#1f9213"
        android:text="Level - 21"
        android:id="@+id/textView17"
        android:textSize="35dp"
        android:layout_centerHorizontal="true"
        android:layout_below="@+id/angry_btn" />

    <GridLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:columnCount="10"
        android:rowCount="9"
        android:id="@+id/gridLayout"
        android:layout_alignRight="@+id/button5"
        android:layout_alignEnd="@+id/button5"
        android:layout_below="@+id/textView17">

        <Space
            android:layout_width="65dp"
            android:layout_height="65dp"
            android:layout_row="0"
            android:layout_column="2" />

        <Space
            android:layout_width="64dp"
            android:layout_height="65dp"
            android:layout_row="1"
            android:layout_column="0" />

        <ToggleButton
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:id="@+id/b1"
            android:background="@drawable/toggleselector_green"
            android:textOff=""
            android:textOn=""
            android:checked="false"
            android:layout_below="@+id/chkState"
            android:layout_centerHorizontal="true"
            android:layout_row="1"
            android:layout_column="2" />

        <ToggleButton
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:id="@+id/b2"
            android:background="@drawable/toggleselector_green"
            android:textOff=""
            android:textOn=""
            android:checked="false"
            android:layout_below="@+id/chkState"
            android:layout_centerHorizontal="true"
            android:layout_row="3"
            android:layout_column="2" />

        <Space
            android:layout_width="32dp"
            android:layout_height="35dp"
            android:layout_row="1"
            android:layout_column="3"
            android:id="@+id/space15" />

        <ToggleButton
            android:layout_width="45dp"
            android:layout_height="45dp"
            android:id="@+id/circle1"
            android:background="@drawable/toggle_circle_on"
            android:textOff=""
            android:textOn=""
            android:checked="false"
            android:layout_below="@+id/chkState"
            android:layout_centerHorizontal="true"
            android:layout_row="1"
            android:layout_column="4" />

        <ToggleButton
            android:layout_width="45dp"
            android:layout_height="45dp"
            android:id="@+id/circle2"
            android:background="@drawable/toggle_circle_on"
            android:textOff=""
            android:textOn=""
            android:checked="false"
            android:layout_below="@+id/chkState"
            android:layout_centerHorizontal="true"
            android:layout_row="1"
            android:layout_column="6" />

        <Space
            android:layout_width="35dp"
            android:layout_height="35dp"
            android:layout_row="2"
            android:layout_column="2"
            android:id="@+id/smallspace" />

        <ToggleButton
            android:layout_width="45dp"
            android:layout_height="45dp"
            android:id="@+id/circle3"
            android:background="@drawable/toggle_circle_on"
            android:textOff=""
            android:textOn=""
            android:checked="false"
            android:layout_below="@+id/chkState"
            android:layout_centerHorizontal="true"
            android:layout_row="3"
            android:layout_column="4"
            android:clickable="false" />

        <ToggleButton
            android:layout_width="45dp"
            android:layout_height="45dp"
            android:id="@+id/circle4"
            android:background="@drawable/toggle_circle_on"
            android:textOff=""
            android:textOn=""
            android:checked="false"
            android:layout_below="@+id/chkState"
            android:layout_centerHorizontal="true"
            android:layout_row="3"
            android:layout_column="6"
            android:clickable="false" />

        <Space
            android:layout_width="35dp"
            android:layout_height="46dp"
            android:layout_row="4"
            android:layout_column="2"
            android:id="@+id/space16" />

        <ToggleButton
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:id="@+id/b3"
            android:background="@drawable/toggleselector_green"
            android:textOff=""
            android:textOn=""
            android:checked="false"
            android:layout_below="@+id/chkState"
            android:layout_centerHorizontal="true"
            android:layout_row="5"
            android:layout_column="4" />

        <Space
            android:layout_width="35dp"
            android:layout_height="35dp"
            android:layout_row="5"
            android:layout_column="5"
            android:id="@+id/space17" />

        <ToggleButton
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:id="@+id/b4"
            android:background="@drawable/toggleselector_green"
            android:textOff=""
            android:textOn=""
            android:checked="false"
            android:layout_below="@+id/chkState"
            android:layout_centerHorizontal="true"
            android:layout_row="5"
            android:layout_column="6" />

    </GridLayout>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="\n\n\n\n\n\n"
        android:id="@+id/textView24"
        android:layout_below="@+id/angry_btn"
        android:layout_alignLeft="@+id/textView17"
        android:layout_alignStart="@+id/textView17" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView25"
        android:layout_below="@+id/textView17"
        android:layout_centerHorizontal="true" />

    <Button
        android:id="@+id/button5"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:background="@drawable/retrybuttongreen"
        android:layout_alignParentTop="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:id="@+id/textView32"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="58dp"
        android:textColor="#be6c22" />

</RelativeLayout>
